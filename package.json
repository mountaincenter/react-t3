{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "prisma generate && next build",
    "db:push": "dotenv -e .env.development.local -- prisma db push",
    "db:migrate": "dotenv -e .env.development.local -- npx prisma migrate deploy",
    "db:status": "dotenv -e .env.development.local -- npx prisma migrate status",
    "db:reset": "dotenv -e .env.development.local -- npx prisma migrate reset --force",
    "db:studio": "dotenv -e .env.development.local -- npx prisma studio",
    "migrate:dev": "dotenv -e .env.development.local -- npx prisma migrate dev --name",
    "dev": "next dev",
    "generate": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "ts": "npx dotenv -e .env.development.local -- node --loader ts-node/esm"
  },
  "ts-node": {
    "esm": true
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.10",
